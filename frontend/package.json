    "fake-indexeddb": "^6.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "msw": "^2.8.6",
    "playwright": "^1.52.0",
    "react-hook-form": "^7.56.4",
    "tailwindcss": "^4",
    "tiny-segmenter": "^0.2.0",
    "typescript": "^5",
    "vitest": "^3.1.4",
    "yup": "^1.6.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
